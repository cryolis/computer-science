{
  "name": "@vanillajs2/compsci",
  "version": "0.6.0",
  "license": "MIT",
  "author": "Evan Plaice <evanplaice@gmail.com> (http://evanplaice.com/)",
  "description": "A repository containing typical reference algorithms & data structures",
  "keywords": [
    "vanilla",
    "esm"
  ],
  "repository": "git@github.com:vanillajs2/compsci.git",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "test": "find . -wholename '**/*.spec.js' -not -path './node_modules/*' | xargs -n1 node --experimental-modules",
    "test:watch": "npx chokidar-cli -i 'node_modules' '**/*.js' -c 'npm test'",
    "lint": "npx semistandard",
    "types": "npx tsc",
    "package": "npx rimraf package && npm pack | tail -n 1 | xargs tar -xf",
    "preversion": "npm test && npm run lint && npm run types",
    "postversion": "git push --follow-tags"
  },
  "dependencies": {},
  "devDependencies": {
    "@types/tape": "^4.2.33",
    "babel-eslint": "^10.0.2",
    "semistandard": "^13.0.1",
    "tape": "^4.11.0"
  },
  "engines": {
    "node": ">=12.0"
  },
  "semistandard": {
    "parser": "babel-eslint",
    "ignore": [
      "index.js"
    ]
  }
}
